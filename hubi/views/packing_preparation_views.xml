<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="view_hubi_orders_packing_preparation_tree" model="ir.ui.view">
		<field name="name">hubi.sale.order.packing.preparation.tree</field>
		<field name="model">sale.order.line</field> 
		<field name="arch" type="xml">
			<tree string="Packing Preparation" default_order="partner_id">
				<field name="partner_id"/>
				<field name="sale_order_id"/>
				<field name="product_id"/>
				<field name="product_uom_qty" string="Qty"/>
				<field name="print_label"/>
				<button name="validation">O.K.</button>
			</tree>
		</field>
	</record>
</odoo>